
@{
    Layout = null;
}
@using AracIhaleSistemi.UI.ViewModels
<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <style>
        body {
            background: #f5f8fa
        }

        media (min-width: 1200px) {
            .container

        {
            max-width: 1200px;
        }
        }
    </style>

    <nav class="navbar sticky-top navbar-expand-lg navbar-dark bg-dark">
        <a class="navbar-brand" href="#">Navbar</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">
                <li class="nav-item active">
                    <a class="nav-link" href="Anasayfa">Anasayfa <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/AdminArea/Kullanici/Index">Kullanıcı İşlemleri</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/AdminArea/Komisyon/Index">Komisyon Tanımlama</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/AdminArea/Noter/Index">Noter Ücreti Tanımlama</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/AdminArea/Paket/Index">Paket Tanımlama</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/AdminArea/Arac/Index">Araç İşlemleri</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="IhaleListeleme">İhale Listeleme</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="Bilgilerim">Bilgilerim</a>
                </li>
            </ul>
            <form class="form-inline my-2 my-lg-0">
                @*<input class="form-control form-control-sm mr-sm-2" type="search" placeholder="Search" aria-label="Search" >*@
                @*<button class="btn btn-sm btn-info my-2 my-sm-0" type="submit">Çıkış Yap</button>*@

                <a class="btn btn-success-sm btn-info my-2 my-sm-0" href="CikisYap" role="button">Çıkış Yap</a>
            </form>
        </div>
    </nav>

    <div class="container">


        <br />
        @RenderBody()

    </div>

    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    @*<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>*@
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/js/bootstrap.min.js" integrity="sha384-a5N7Y/aK3qNeh15eJKGWxsqtnX/wWdSZSKp+81YjTmS15nvnvxKHuzaWwXHDli+4" crossorigin="anonymous"></script>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
